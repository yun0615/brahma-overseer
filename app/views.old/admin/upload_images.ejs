<%- include('../_layout/admin_header'); %>

        <!-- Content start -->
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Images - Upload</h1>
                </div>
            </div>

            <div class="row">
                <table class="col-md-8 col-lg-8" style="font-size:18px;margin-left:20px">
                    <tr>
                        <td>Images Name： </td>
                        <td><input type="text" class="form-control" style="display:inline"></td>
                    </tr>
                    <tr>
                        <td>VM NAMESPACE：</td>
                        <td><input type="text" class="form-control" style="display:inline" placeholder="e.g. : Android_VM_"></td>
                    </tr>
                    <tr>
                        <td>Upload File：</td>
                        <td><input type="file"></td>
                    </tr>
                    <tr>
                        <td>Operation System：</td>
                        <td>
                            <select name="" class="form-control" id="">
                            	<option value="" selected="true">Android</option>
                                <option value="">iOS</option>
                            </select>
                        </td>
                    </tr>
                    <tr>
                        <td>Bits：</td>
                        <td>
                            <select name="" class="form-control" id="">
                            	<option value="" selected="true">32</option>
                                <option value="">64</option>
                            </select>
                        </td>
                    </tr>
                    <tr>
                        <td>Version：</td>
                        <td><input type="text" class="form-control"></td>
                    </tr>
                    <tr>
                        <td colspan="2">Introduction：<br />
                            <textarea name="" id="" style="width:100%;height:100px"></textarea>
                        </td>
                    </tr>
                    <tr>
                        <tr>
                            <td>Count：</td>
                            <td>
                                <input type="number" min='1' style="width:20%" class="form-control">
                            </td>
                        </tr>
                        <td colspan="2">
                            <a href="#" class="btn btn-success col-md-offset-5 col-lg-offset-5" style="margin-right:20px">OK</a>
                            <a href="" onclick="javascript:history.go(-1)" class="btn btn-default">Back</a>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <!-- End of Content -->

    </div>
	<!-- end of wrapper -->

	

<%- include('../_layout/admin_footer'); %>

